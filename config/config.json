{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "banner-wp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bannerWp/BannerWpWebPart.js",
          "manifest": "./src/webparts/bannerWp/BannerWpWebPart.manifest.json"
        }
      ]
    },
    "sticky-bar-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/stickyBar/StickyBarWebPart.js",
          "manifest": "./src/webparts/stickyBar/StickyBarWebPart.manifest.json"
        }
      ]
    },
    "html-5-canvas-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/html5Canvas/Html5CanvasWebPart.js",
          "manifest": "./src/webparts/html5Canvas/Html5CanvasWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BannerWpWebPartStrings": "lib/webparts/bannerWp/loc/{locale}.js",
    "StickyBarWebPartStrings": "lib/webparts/stickyBar/loc/{locale}.js",
    "Html5CanvasWebPartStrings": "lib/webparts/html5Canvas/loc/{locale}.js"
  }
}
